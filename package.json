{
  "name": "spa-store",
  "version": "1.0.0",
  "description": "single page application javascript vanilla",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "webpack": "export NODE_OPTIONS=--openssl-legacy-provider",
    "start": "webpack-dev-server --open --mode development",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:parallel": "cypress-parallel -s cy:run -t 3 -d 'cypress/e2e/*.js'",
    "cy:test_record": "npx cypress run  --spec ./cypress/e2e/*.cy.js "
  },
  "keywords": [
    "SPA",
    "javascritp",
    "vanilla"
  ],
  "author": "den velez den.velez@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.2",
    "cypress": "^13.8.1",
    "eslint-plugin-cypress": "^3.1.1",
    "html-webpack-plugin": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "cypress-parallel": "^0.14.0",
    "mochawesome": "^7.1.3"
  }
}
